
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>
        {% block title %}Welcome Rocehllengues{% endblock %}
    </title>
    <link href="{{ asset('img/logo.png') }}" rel="icon">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@0.5.3/dist/css/axentix.min.css"> {% block stylesheets %}{% endblock %}
    <link rel="stylesheet" href="{{ asset('css/styles.css') }}">
</head>
<body class="layout with-sidenav">
<header>


    <nav class="navbar primary">
        <a  class="navbar-brand">Bienvenue chez la Rochellengues</a>
        <div class="navbar-menu ml-auto">
            <a class="navbar-link" href="{{ path('home') }}">Accueil</a>
            <a class="navbar-link" href="{{ path('user') }}">Retour</a>
            <a class="navbar-link"  href="{{ path('app_logout') }}">Logout</a>


        </div>
    </nav>
</header>
<div id="example-sidenav" class="sidenav large fixed white">
    <div class="sidenav-header">
        <button data-target="example-sidenav" class="sidenav-trigger">
            <i class="fas fa-times"></i>
        </button>
        <img class="logo dropshadow-1" src="{{ asset('img/logouniversitÃ©.jpg') }}" width="60" height="90" alt="Logo"/>
    </div>
    <a href="{{ path('user') }}" class="sidenav-link active">Mon profil</a>
    <a href="{{ path('annonces_index') }}" class="sidenav-link">Mes annonces</a>
    <a href="{{ path('sejours_index') }}" class="sidenav-link">Sejour</a>
    <a href="#" class="sidenav-link">Mes evenements</a>
</div>
<main>
    {% block body %}




    {% endblock %}

</main>




<script src="https://cdn.jsdelivr.net/npm/axentix@0.5.3/dist/js/axentix.min.js"></script>
{% block javascripts %}{% endblock %}
<script>
    var sidenav = new Sidenav('#example-sidenav');
</script>
<script src="{{ asset('js/scripts.js') }}"></script>
</body>
</html>